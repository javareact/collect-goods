{
  "name": "花卷云自营商城辅助工具",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "花卷云自营商城辅助工具",
  "author": "http://www.dhsoft.cn/",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/jquery-1.7.2.min.js",
        "js/contentscript.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "js/jquery-1.7.2.min.js",
      "js/background.js"
    ]
  }
}
